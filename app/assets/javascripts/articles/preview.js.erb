$(function(){
  // コードをハイライトするためにオリジナルのrendererを作る

  // markdownの設定
  marked.setOptions({
    langPrefix: '',
    breaks: true,
    sanitize: true
  });

  $("#article_body").keyup(function() {
    var src = $(this).val();
    var preview = marked(src);
    $("#preview--container").html(`<div>${preview}</div>`);
  });
});
