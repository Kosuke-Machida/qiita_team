<h1>Users#show</h1>
  <p>name:<%= @user.name %></p>
  <p>email:<%= @user.email %></p>
  <div>
    <%= button_to "user編集画面へ", edit_user_registration_path, method: :get %>
  </div>
  <div>Stocks<br>
  <% if @articles.nil? == false %>
    <% @articles.each do |article| %>
      <%= link_to article.title, article_path(article) %>
      <%= button_to "削除", article_stock_path(article, Stock.where(["user_id = ? and article_id = ?", @user.id, article.id])), :method => :delete %><br>
    <% end %>
  <% end %>
  <% if @articles.nil? %>
    <div>stockしている投稿はありません</div>
  <% end %>
</div>
<p>Find me in app/views/users/show.html.erb</p>
