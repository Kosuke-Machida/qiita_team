<h1>Articles#new</h1>
<%= form_for @article do |f| %>
   <div>
     <p><%= f.text_field :title %>
   </div>
   <div>
     <%= f.text_field :body %>
   </div>
   <div>
     <%= f.label :tag_list, "Tags (separated by commas)" %><br>
     <%= f.text_field :tag_list %>
  </div>
   <%= f.hidden_field :user_id, value: current_user.id %>
   <%= f.submit "送信" %>
<% end %>
<p>Find me in app/views/articles/new.html.erb</p>
